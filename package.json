{
  "name": "material-community-components",
  "version": "5.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "build:lib:scss": "scss-bundle -c scss-bundle.config.json",
    "build:docs": "ng build --prod --base-href https://tiaguinho.github.io/material-community-components/",
    "docs:deploy": "npx ngh --dir=dist/material-community-components",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "packagr": "ng-packagr -p src/lib/package.json && yarn build:lib:scss && cp README.md src/lib/dist/"
  },
  "private": true,
  "peerDependencies": {
    "core-js": "^3.1.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.1",
    "@angular-devkit/core": "9.0.1",
    "@angular/animations": "^9.0.0",
    "@angular/cdk": "^8.0.1",
    "@angular/cli": "^9.0.1",
    "@angular/common": "^9.0.0",
    "@angular/compiler": "^9.0.0",
    "@angular/compiler-cli": "^9.0.0",
    "@angular/core": "^9.0.0",
    "@angular/forms": "^9.0.0",
    "@angular/language-service": "^9.0.0",
    "@angular/material": "^8.0.1",
    "@angular/platform-browser": "^9.0.0",
    "@angular/platform-browser-dynamic": "^9.0.0",
    "@angular/router": "^9.0.0",
    "@types/jasmine": "~3.3.13",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "conventional-changelog": "^3.1.8",
    "core-js": "^3.1.4",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^2.4.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^9.0.0",
    "protractor": "~5.4.0",
    "rxjs": "^6.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.17.0",
    "typescript": "3.7.5",
    "uglify-es": "3.3.8",
    "validate-commit-msg": "^2.14.0",
    "zone.js": "~0.10.2"
  },
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert",
        "release",
        "ci",
        "build"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100,
      "subjectPattern": ".+",
      "subjectPatternErrorMsg": "subject does not match subject pattern!",
      "helpMessage": ""
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "scss-bundle": "^2.5.1",
    "tslib": "^1.10.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "validate-commit-msg"
    }
  }
}
